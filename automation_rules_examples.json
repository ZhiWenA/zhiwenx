{
  "xiaohongshu_search": {
    "name": "小红书搜索AdventureX",
    "description": "打开小红书并搜索关键词AdventureX",
    "steps": [
      {
        "type": "launchApp",
        "description": "启动小红书",
        "appPackage": "com.xingin.xhs",
        "timeout": 5000
      },
      {
        "type": "waitForElement",
        "description": "等待搜索图标出现",
        "selector": {
          "byResourceId": "com.xingin.xhs:id/search_icon",
          "byContentDescription": "搜索"
        },
        "timeout": 10000
      },
      {
        "type": "click",
        "description": "点击搜索图标",
        "selector": {
          "byResourceId": "com.xingin.xhs:id/search_icon"
        },
        "timeout": 3000
      },
      {
        "type": "waitForElement",
        "description": "等待搜索框出现",
        "selector": {
          "byClassName": "android.widget.EditText"
        },
        "timeout": 5000
      },
      {
        "type": "input",
        "description": "输入搜索关键词",
        "selector": {
          "byClassName": "android.widget.EditText"
        },
        "inputText": "AdventureX",
        "timeout": 3000
      },
      {
        "type": "keyEvent",
        "description": "按回车键执行搜索",
        "keyCode": 66,
        "timeout": 1000
      }
    ]
  },
