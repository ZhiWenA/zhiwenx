  "generic_app_template": {
    "name": "通用应用搜索模板",
    "description": "通用的应用搜索流程模板，可以根据具体应用调整选择器",
    "steps": [
      {
        "type": "launchApp",
        "description": "启动目标应用",
        "appPackage": "com.example.app",
        "timeout": 5000
      },
      {
        "type": "waitForElement",
