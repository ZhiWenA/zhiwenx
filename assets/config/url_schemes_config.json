        "lat": {
          "name": "lat",
          "description": "纬度",
          "type": "number",
          "required": true,
          "url_encode": false,
          "min": -90,
          "max": 90
        },
        "lon": {
          "name": "lon",
          "description": "经度",
          "type": "number",
          "required": true,
          "url_encode": false,
          "min": -180,
          "max": 180
